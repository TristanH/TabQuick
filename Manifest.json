{
  "manifest_version": 2,

  "name": "TabQuick",
  "description": "Saves your current tabs into a bookmark folder whenever you want, as much as you want.",
  "version": "1.0",
	
  "background":{
	"scripts": ["eventPage.js"],
	"persistent": false 
  },
	
  "permissions": [
	"bookmarks",
	"tabs",
	"storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
